<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="prof.magnitos.speedytask" >

    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />

    <!-- <uses-permission android:name="android.permission.RESTART_PACKAGES"/> -->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="prof.magnitos.speedytask.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="prof.magnitos.speedytask.ChartActivity"
            android:label="@string/app_name" >

        </activity>
        <activity
            android:name="prof.magnitos.speedytask.TabActivity"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name="prof.magnitos.speedytask.SplashActiv"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>

        </activity>

    </application>

</manifest>
